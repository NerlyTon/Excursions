<%= form_for [@excursion, @booking] do |f|%>
    <%= f.label :date%><br>
    <%= f.date_select :date, order: [:month, :day, :year], start_year: Date.today.year%><br>
    Number of People:<br><%= f.number_field :num_of_people%>
    <%= f.hidden_field :excursion_id, value: @excursion.id%>
    <%= f.hidden_field :user_id, value: current_user.id%><br><br>
    <%= f.submit %>
    <% end %>